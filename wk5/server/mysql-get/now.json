{
  "version": 2,
  "name": "wmdd-get-w5",
  "env": {
    "DBHOST": "your_aws_endpoint",
    "DBPORT": "3306",
    "DBUSER": "your_username",
    "DBPASSWORD": "your_password",
    "DBNAME": "blog"
  },
  "builds": [
    { "src": "index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
